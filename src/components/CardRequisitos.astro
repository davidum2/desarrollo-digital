---
interface Props {
	title: string;
	
}

const {title } = Astro.props;
---


<div class="md:p-6  mb-4 bg-white  border border-gray-200 shadow-black dark:shadow-white rounded-lg shadow-sm dark:bg-gray-800 dark:border-gray-700 ">
    <div>
        <h5 class="mb-2 text-center text-2xl font-semibold tracking-tight text-gray-900 dark:text-white">{title}</h5>
    </div>
	<slot/>
        
</div>
